fn main() -> int {
	mut fugo : int = 0;
	const hoge : int = 1000;
	const test : bool = false;
	if (test) {
		fugo = 100;
	} else {
		fugo = 200;
	}

	mut idx : int = 0;
	while (idx < 10) {
		idx = idx + 1;
	}

	return max(idx, 0);
